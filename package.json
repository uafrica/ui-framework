{
  "version": "2.0.98",
  "name": "bob-group-ui-framework",
  "license": "BSD-3-Clause",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "description": "A custom set of UI components used by Bob Group, based on TailwindCSS and Headless UI",
  "repository": "uafrica/ui-framework",
  "bugs": {
    "url": "https://github.com/uafrica/ui-framework/issues",
    "email": "cornel@bob.co.za"
  },
  "author": {
    "name": "Cornel Rautenbach",
    "email": "cornel@bob.co.za"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build && yarn build-tailwind",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "yarn build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build-tailwind": "NODE_ENV=production npx tailwindcss -o ./dist/tailwind.css --minify"
  },
  "peerDependencies": {
    "@react-google-maps/api": "^2.18.1",
    "react": ">=16",
    "react-router-dom": "5.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "es5"
  },
  "module": "dist/bob-group-ui-framework.esm.js",
  "size-limit": [
    {
      "path": "dist/bob-group-ui-framework.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/bob-group-ui-framework.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@size-limit/preset-small-lib": "^8.2.4",
    "@storybook/addon-essentials": "^7.0.7",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^7.0.7",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addons": "^7.0.7",
    "@storybook/cli": "^7.0.7",
    "@storybook/react": "^7.0.7",
    "@types/google.maps": "^3.43.3",
    "@types/lodash": "^4.14.195",
    "@types/node": "15.0.3",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/uuid": "^9.0.1",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.2",
    "@types/leaflet": "^1.9.8",
    "husky": "^8.0.3",
    "postcss": "^8.4.27",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "size-limit": "^8.2.4",
    "tailwindcss": "^3.3.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "country-flag-icons": "^1.5.7",
    "leaflet": "^1.9.4",
    "leaflet-gesture-handling": "^1.2.2",
    "react-hook-form": "^6.14.0",
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@headlessui/react": "^1.7.14",
    "@popperjs/core": "^2.11.7",
    "@react-google-maps/api": "^2.18.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.4",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "^0.5.9",
    "@types/react-dom": "17.0.5",
    "@types/react-router-dom": "5.1.7",
    "detect-browser": "^5.3.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "react-content-loader": "^6.2.1",
    "react-error-boundary": "^4.0.10",
    "react-popper": "^2.3.0",
    "react-router-dom": "5.2.0",
    "rollup-plugin-postcss": "^4.0.2",
    "sass": "^1.64.2",
    "uuid": "^9.0.0"
  }
}
