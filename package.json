    "@react-google-maps/api": "^2.18.1",
    "craco": "^0.0.3",
    "detect-browser": "^5.3.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "moment": "^2.29.4",
    "node-sass": "8",
    "prettier": "2.3.0",
    "pretty-quick": "3.1.0",
    "react": "^18.2.0",
    "react-content-loader": "^6.2.1",
